                    [ "import { members } from \"wix-groups-backend\";",
                      "",
                      "export function addGroupMembers() {",
                      "  const identifiers = {",
                      "    groupId: \"a string\",",
                      "    memberIds: [\"a string\"]",
                      "  };",
                      "  const options = {",
                      "    suppressAuth: true",
                      "  };",
