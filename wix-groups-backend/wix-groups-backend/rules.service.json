        "docs":
          { "summary": "Replace existing or create new group rules (all existing rules will be replaced by provides ones)",
            "description":
              [ "Groups managers and group admins have access to this functionality." ],
            "links": [],
            "examples":
              [ { "title": "basic usage",
                  "body":
                    [ "import { rules } from \"wix-groups-backend\";",
                      "",
                      "export function createOrReplaceGroupRules() {",
                      "  const createOrReplaceGroupRulesRequest = {",
                      "    groupId: \"a string\",",
                      "    rules: {",
                      "      rules: [",
                      "        {",
                      "          title: \"a string\",",
                      "          description: \"a string\"",
                      "        }",
                      "      ]",
                      "    }",
                      "  };",
                      "  const options = {",
                      "    suppressAuth: true",
                      "  };",
                      "",
                      "  return rules",
                      "    .createOrReplaceGroupRules(createOrReplaceGroupRulesRequest, options)",
                      "    .then((response) => {",
                      "      console.log(response);",
                      "    })",
                      "    .catch((error) => {",
                      "      console.error(error);",
                      "    });",
                      "}" ],
                  "extra":
                    {  } } ],
